{
  "name": "Urchin",
  "description": "Schedules,Actions, Scrape and Intercept",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions":[
    "tabs","storage","scripting","activeTab","cookies",
    "webRequest","alarms","management","webNavigation","pageCapture","offscreen","gcm"
  ],
  "web_accessible_resources": [{
    "resources": ["icons/*","images/*"],
    "matches": ["<all_urls>"]
  }],
  "host_permissions": ["<all_urls>"],
  "background": {
  "service_worker": "background.js"
  },
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  },
  "action":{
      "default_icon":{
        "16":"/icons/URCHIN icon.png",
        "48":"/icons/URCHIN icon.png"
      },
      "default_popup":"popup.html"
  },
  "content_scripts":[{
    "matches":["<all_urls>"],
    "js":["jquery.js","script.js"]
    }]
}